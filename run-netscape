#! /bin/sh
# $Id: run-netscape,v 1.8 1999/11/01 09:37:06 friedman Exp $

HOME=$HOME/etc/www/netscape
export HOME

prefs=$HOME/.netscape/preferences.js
if ls -d $HOME/.netscape/lock > /dev/null 2>&1; then
  :
elif [ -f "${prefs}.append" ] && [ -f "$prefs" ]; then
  cat "${prefs}.append" >> "$prefs"
fi

exec netscape ${1+"$@"}

# eof
