#! /bin/sh
# $Id: run-netscape,v 1.4 1997/09/08 03:59:54 friedman Exp $

case "$1" in
  -* ) ;;
  * )
    ver="$1"; shift
    nam="$1"; shift

    case "$ver" in
      '' | any ) : ;;
      *.* )
        MOZVERSION="$ver"
        export MOZVERSION
       ;;
      * )
        MOZMAJORVERSION="$ver"
        export MOZMAJORVERSION
       ;;
    esac

    case "$nam" in
      '' | normal ) : ;;
      * )
        MOZNAME="$nam"
        MOZPROG="$nam"
        export MOZNAME MOZPROG
       ;;
    esac
   ;;
esac

HOME=$HOME/etc/www/netscape
export HOME

exec netscape-fe ${1+"$@"}

# eof
