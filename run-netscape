#! /bin/sh
# $Id: run-netscape,v 1.3 1997/06/10 10:18:26 friedman Exp $

ver="$1"; shift
nam="$1"; shift

case "$ver" in
  '' | any ) : ;;
  *.* )
    MOZVERSION="$ver"
    export MOZVERSION
   ;;
  * )
    MOZMAJORVERSION="$ver"
    export MOZMAJORVERSION
   ;;
esac

case "$nam" in
  '' | normal ) : ;;
  * )
    MOZNAME="$nam"
    MOZPROG="$nam"
    export MOZNAME MOZPROG
   ;;
esac

HOME=$HOME/etc/www/netscape
export HOME

# case "$ver" in
#   4* ) HOME="$HOME/dogbert" ;;
# esac

exec netscape-fe ${1+"$@"}

# eof
